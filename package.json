{
  "name": "job-digest-bot",
  "version": "1.0.0",
  "description": "Automated job alert curation and notification system",
  "main": "dist/api/main.js",
  "scripts": {
    "build": "npx nx run-many --target=build --all",
    "build:api": "npx nx build api",
    "build:web": "npx nx build web",
    "start": "npx nx run-many --target=start --all",
    "start:api": "npx nx start api",
    "start:web": "npx nx start web",
    "dev": "npx nx run-many --target=dev --all --parallel",
    "dev:api": "npx nx serve api",
    "dev:web": "npx nx dev web",
    "lint": "npx nx run-many --target=lint --all",
    "lint:api": "npx nx lint api",
    "lint:web": "npx nx lint web",
    "lint:fix": "npx nx run-many --target=lint --all --fix",
    "format": "npx nx format:write",
    "format:check": "npx nx format:check",
    "test": "npx nx run-many --target=test --all",
    "test:api": "npx nx test api",
    "test:web": "npx nx test web",
    "type-check": "npx nx run-many --target=type-check --all",
    "type-check:api": "tsc --noEmit --project apps/api/tsconfig.app.json",
    "type-check:web": "tsc --noEmit --project apps/web/tsconfig.json",
    "generate-gmail-token": "node apps/api/src/utils/gmail-token-generator.js",
    "generate-api-types": "npm run build:api && node scripts/generate-api-types.js",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:studio": "npx prisma studio",
    "test:prisma": "npx tsx scripts/test-prisma.ts",
    "nx:graph": "npx nx graph",
    "nx:reset": "npx nx reset",
    "postinstall": "npx nx --version"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.85.3",
    "bullmq": "^5.56.10",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "next": "~15.2.4",
    "next-auth": "^4.24.11",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.64.0",
    "openai": "^4.28.4",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.14.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.62.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.3.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nx/devkit": "21.3.11",
    "@nx/esbuild": "21.3.11",
    "@nx/eslint": "^21.3.11",
    "@nx/js": "^21.3.11",
    "@nx/next": "^21.3.11",
    "@nx/node": "^21.3.11",
    "@nx/playwright": "^21.3.11",
    "@nx/react": "^21.3.11",
    "@playwright/test": "^1.54.2",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.17",
    "@types/node-cron": "^3.0.11",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "lucide-react": "^0.539.0",
    "nx": "^21.3.11",
    "openapi-typescript": "^7.9.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.7.1",
    "typescript": "~5.8.2"
  },
  "engines": {
    "node": ">=18"
  }
}
